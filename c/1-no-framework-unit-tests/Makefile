CC      := clang
CFLAGS  := -Wall -Wextra -std=c2x -pedantic
LDFLAGS := 

.DEFAULT_GOAL: bin/main


bin/main : obj/main.o
	@mkdir -p bin
	${CC} ${LDFLAGS} -o bin/main obj/main.o

obj/main.o : src/main.c
	@mkdir -p $(dir obj/main.o)
	${CC} ${CFLAGS} -Iinclude -c src/main.c -o obj/main.o

.PHONY : clean
clean :
	rm -rf obj

.PHONY : purge
purge : clean
	rm -rf bin
