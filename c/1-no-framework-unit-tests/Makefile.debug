CC      := clang
CFLAGS  := -Wall -Wextra -std=c2x -pedantic -g3 -glldb
LDFLAGS := 

.DEFAULT_GOAL: bin/debug


bin/debug : debug/main.o
	@mkdir -p bin
	${CC} ${LDFLAGS} -o bin/debug debug/main.o

debug/main.o : src/main.c
	@mkdir -p $(dir debug/main.o)
	${CC} ${CFLAGS} -Iinclude -c src/main.c -o debug/main.o

.PHONY : clean
clean :
	rm -rf debug

.PHONY : purge
purge : clean
	rm -rf bin
